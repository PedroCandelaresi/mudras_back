#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules:/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules:/home/candelaresi/Proyectos/mudras/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.28_mysql2@3.16.2_ts-node@10.9.2_@types+node@24.10.9_typescript@5.9.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
